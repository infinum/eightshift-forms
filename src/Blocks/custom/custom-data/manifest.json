{
	"$schema": "https://raw.githubusercontent.com/infinum/eightshift-frontend-libs/develop/schemas/block.json",
	"blockName": "custom-data",
	"title": "Custom data",
	"description" : "A field that displays custom data from a data source",
	"category": "eightshift-forms",
	"icon": {
		"src": "esf-custom-data"
	},
	"keywords": [
		"customData"
	],
	"example": {
	},
	"attributes": {
		"customDataUniqueId": {
			"type": "string"
		},
		"customDataFieldType": {
			"type": "string"
		},
		"customDataData": {
			"type": "string"
		},
		"customDataId": {
			"type": "string"
		},
		"customDataFormPostId": {
			"type": "string"
		},
		"customDataServerSideRender": {
			"type": "boolean",
			"default": false
		}
	},
	"parent": [
		"eightshift-forms/form"
	],
	"components": {
		"select": "select",
		"checkboxes": "checkboxes",
		"radios": "radios"
	},
	"options": {
		"customDataFieldType": [
			{
				"label": "Select",
				"value": "select"
			},
			{
				"label": "Checkbox",
				"value": "checkboxes"
			},
			{
				"label": "Radio",
				"value": "radios"
			}
		]
	}
}
