{
	"$schema": "https://raw.githubusercontent.com/infinum/eightshift-frontend-libs/develop/schemas/block.json",
	"blockName": "form-selector",
	"title": "Form selector",
	"description" : "Choose the form you want to display",
	"category": "eightshift-forms",
	"icon": {
		"src": "esf-form-picker"
	},
	"keywords": [
		"forms"
	],
	"hasInnerBlocks": true,
