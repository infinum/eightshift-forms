{
	"$schema": "https://raw.githubusercontent.com/infinum/eightshift-frontend-libs/develop/schemas/block.json",
	"blockName": "forms",
	"title": "Form",
	"description" : "Allows selecting the form that should be displayed",
	"category": "eightshift-forms",
	"icon": {
		"src": "esf-form"
	},
	"keywords": [
		"forms"
	],
	"attributes": {
		"formsFormPostId": {
			"type": "string"
		},
		"formsFormPostIdRaw": {
			"type": "object"
		},
		"formsConditionalTagsRulesForms": {
			"type": "array"
		},
		"formsConditionalTagsRules": {
			"type": "array"
		},
		"formsFormGeolocation": {
			"type": "array"
		},
		"formsFormGeolocationAlternatives": {
			"type": "array",
			"items": {
				"type": "object"
			},
			"default": []
		},
		"formsStyle": {
			"type": "array",
			"items": {
				"type": "string"
			},
			"default": []
		},
		"formsVariation": {
			"type": "array",
			"default": []
		},
		"formsVariationData": {
			"type": "object",
			"default": {}
		},
		"formsVariationDataFiles": {
			"type": "array",
			"default": []
		},
		"formsFormDataTypeSelector": {
			"type": "string"
		},
		"formsCustomName": {
			"type": "string"
		},
		"formsAttrs": {
			"type": "object"
		},
		"formsResultsOutputUse": {
			"type": "boolean",
			"default": false
		},
		"formsResultsOutputData": {
			"type": "array"
		},
		"formsDownloads": {
			"type": "array",
			"items": {
				"type": "object"
			},
			"default": []
		},
		"formsSuccessRedirectVariation": {
			"type": "string"
		},
		"formsSuccessRedirectVariationUrl": {
			"type": "string"
		},
		"formsSuccessRedirectVariationUrlTitle": {
			"type": "string"
		}
	},
	"components": {
		"conditionalTags": "conditionalTags",
		"progressBar": "progressBar"
	}
}
