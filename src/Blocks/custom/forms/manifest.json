{
	"$schema": "https://raw.githubusercontent.com/infinum/eightshift-frontend-libs/develop/schemas/block.json",
	"blockName": "forms",
	"title": "Eightshift Forms",
	"description" : "Allows selecting the form that should be displayed",
	"category": "eightshift-forms",
	"icon": {
		"src": "esf-form"
	},
	"keywords": [
		"forms"
	],
	"attributes": {
		"formsFormPostId": {
			"type": "string"
		},
		"formsFormGeolocation": {
			"type": "array"
		},
		"formsFormGeolocationAlternatives": {
			"type": "array",
			"items": {
				"type": "object"
			},
			"default": []
		},
		"formsStyle": {
			"type": "string"
		},
		"formsFormDataTypeSelector": {
			"type": "string"
		},
		"formsServerSideRender": {
			"type": "boolean",
			"default": false
		}
	},
	"postType": "eightshift-forms",
	"attributesSsr": [
		"formsFormPostId",
		"formsStyle"
	]
}
