{
	"$schema": "https://raw.githubusercontent.com/infinum/eightshift-frontend-libs/develop/schemas/component.json",
	"componentName": "checkbox",
	"title": "Checkbox",
	"componentClass": "es-checkbox",
	"componentJsSingleSubmitClass": "js-es-block-single-submit",
	"components": {
		"error": "error"
	},
	"example": {
		"attributes": {
			"checkboxLabel": "Label",
			"checkboxValue": "Value",
			"checkboxName": "Name",
			"checkboxIsChecked": false,
			"checkboxIsDisabled": false,
			"checkboxIsReadOnly": false,
			"checkboxIsRequired": false
		}
	},
	"attributes": {
		"checkboxLabel": {
			"type": "string"
		},
		"checkboxHideLabelText": {
			"type": "boolean",
			"default": false
		},
		"checkboxValue": {
			"type": "string"
		},
		"checkboxUncheckedValue": {
			"type": "string"
		},
		"checkboxName": {
			"type": "string"
		},
		"checkboxIsChecked": {
			"type": "boolean",
			"default": false
		},
		"checkboxIsDisabled": {
			"type": "boolean",
			"default": false
		},
		"checkboxIsReadOnly": {
			"type": "boolean",
			"default": false
		},
		"checkboxIsRequired": {
			"type": "boolean",
			"default": false
		},
		"checkboxTracking": {
			"type": "string"
		},
		"checkboxFieldType": {
			"type": "string",
			"default": "fieldset"
		},
		"checkboxSingleSubmit": {
			"type": "boolean",
			"default": false
		},
		"checkboxAttrs": {
			"type": "object"
		},
		"checkboxAsToggle": {
			"type": "boolean",
			"default": false
		},
		"checkboxAsToggleSize": {
			"type": "string"
		},
		"checkboxDisabledOptions": {
			"type": "array"
		}
	}
}
