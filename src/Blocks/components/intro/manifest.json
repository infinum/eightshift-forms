{
	"$schema": "https://raw.githubusercontent.com/infinum/eightshift-frontend-libs/develop/schemas/component.json",
	"componentName": "intro",
	"title": "Intro",
	"componentClass": "es-intro",
	"attributes": {
		"introTitle": {
			"type": "string"
		},
		"introTitleSize": {
			"type": "string",
			"default": "default"
		},
		"introSubtitle": {
			"type": "string"
		},
		"introIsHighlighted": {
			"type": "boolean",
			"default": false
		},
		"introIsHighlightedImportant": {
			"type": "boolean",
			"default": false
		},
		"introIsHeading": {
			"type": "boolean",
			"default": false
		}
	}
}
