{
	"$schema": "https://raw.githubusercontent.com/infinum/eightshift-frontend-libs/develop/schemas/component.json",
	"componentName": "select",
	"title": "Select",
	"componentClass": "es-select",
	"componentCustomJsClass": "js-es-block-form-custom",
	"componentJsSingleSubmitClass": "js-es-block-form-single-submit",
	"example": {
		"attributes": {
			"selectContent": "",
			"selectName": "Name",
			"selectIsDisabled": false,
			"selectIsRequired": false
		}
	},
	"components": {
		"field": "field",
		"conditionalTags": "conditionalTags"
	},
	"attributes": {
		"selectContent": {
			"type": "string"
		},
		"selectName": {
			"type": "string"
		},
		"selectIsDisabled": {
			"type": "boolean",
			"default": false
		},
		"selectIsRequired": {
			"type": "boolean",
			"default": false
		},
		"selectTracking": {
			"type": "string"
		},
		"selectSingleSubmit": {
			"type": "boolean",
			"default": false
		},
		"selectMeta": {
			"type": "string"
		},
		"selectAttrs": {
			"type": "object"
		},
		"selectUseDynamic": {
			"type": "boolean",
			"default": false
		},
		"selectDynamicValue": {
			"type": "string"
		},
		"selectDisabledOptions": {
			"type": "array"
		},
		"selectUseSearch": {
			"type": "boolean",
			"default": true
		}
	}
}
