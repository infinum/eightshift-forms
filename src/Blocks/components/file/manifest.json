{
	"$schema": "https://raw.githubusercontent.com/infinum/eightshift-frontend-libs/develop/schemas/component.json",
	"componentName": "file",
	"title": "file",
	"componentClass": "es-file",
	"example": {
		"attributes": {
			"fileName": "Name",
			"fileAccept": "Accept",
			"fileIsMultiple": false,
			"fileIsRequired": false
		}
	},
	"components": {
		"field": "field",
		"conditionalTags": "conditionalTags"
	},
	"attributes": {
		"fileName": {
			"type": "string"
		},
		"fileAccept": {
			"type": "string"
		},
		"fileMinSize": {
			"type": "string"
		},
		"fileMaxSize": {
			"type": "string"
		},
		"fileIsMultiple": {
			"type": "boolean",
			"default": false
		},
		"fileIsRequired": {
			"type": "boolean",
			"default": false
		},
		"fileTracking": {
			"type": "string"
		},
		"fileCustomInfoText": {
			"type": "string"
		},
		"fileCustomInfoTextUse": {
			"type": "boolean",
			"default": true
		},
		"fileCustomInfoButtonText": {
			"type": "string"
		},
		"fileMeta": {
			"type": "string"
		},
		"fileAttrs": {
			"type": "object"
		},
		"fileDisabledOptions": {
			"type": "array"
		}
