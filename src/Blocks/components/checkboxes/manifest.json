{
	"$schema": "https://raw.githubusercontent.com/infinum/eightshift-frontend-libs/develop/schemas/component.json",
	"componentName": "checkboxes",
	"title": "checkboxes",
	"componentClass": "es-checkboxes",
	"components": {
		"field": "field",
		"conditionalTags": "conditionalTags"
	},
	"attributes": {
		"checkboxesContent": {
			"type": "string"
		},
		"checkboxesName": {
			"type": "string"
		},
		"checkboxesFieldType": {
			"type": "string",
			"default": "fieldset"
		},
		"checkboxesIsRequired": {
			"type": "boolean",
			"default": false
		},
		"checkboxesIsRequiredCount": {
			"type": "integer",
			"default": 1
		},
		"checkboxesMeta": {
			"type": "string"
		},
		"checkboxesDisabledOptions": {
			"type": "array"
		},
		"checkboxesIsToggles": {
			"type": "boolean",
			"default": false
		},
		"checkboxesTypeCustom": {
			"type": "string"
		},
		"checkboxesFieldAttrs": {
			"type": "object"
		}
	},
	"options": {
		"checkboxesIsRequiredCount": {
			"min":  1,
			"step": 1
		}
	}
}
