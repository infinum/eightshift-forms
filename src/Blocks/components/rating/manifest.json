{
	"$schema": "https://raw.githubusercontent.com/infinum/eightshift-frontend-libs/develop/schemas/component.json",
	"componentName": "rating",
	"title": "Rating",
	"componentClass": "es-rating",
	"example": {
		"attributes": {
			"ratingName": "Name",
			"ratingValue": "Value",
			"ratingIsDisabled": false,
			"ratingIsReadOnly": false,
			"ratingIsRequired": false
		}
	},
	"components": {
		"field": "field",
		"conditionalTags": "conditionalTags"
	},
	"attributes": {
		"ratingName": {
			"type": "string"
		},
		"ratingAmount": {
			"type": "string",
			"default": "5"
		},
		"ratingValue": {
			"type": "string"
		},
		"ratingTypeCustom": {
			"type": "string"
		},
		"ratingIsDisabled": {
			"type": "boolean",
			"default": false
		},
		"ratingUseLabelAsPlaceholder": {
			"type": "boolean",
			"default": false
		},
		"ratingIsReadOnly": {
			"type": "boolean",
			"default": false
		},
		"ratingIsRequired": {
			"type": "boolean",
			"default": false
		},
		"ratingTracking": {
			"type": "string"
		},
		"ratingMeta": {
			"type": "string"
		},
		"ratingAttrs": {
			"type": "object"
		},
		"ratingFieldAttrs": {
			"type": "object"
		},
		"ratingDisabledOptions": {
			"type": "array"
		},
		"ratingSingleSubmit": {
			"type": "boolean",
			"default": false
		}
	}
}
