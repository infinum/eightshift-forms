{
	"$schema": "https://raw.githubusercontent.com/infinum/eightshift-frontend-libs/develop/schemas/component.json",
	"componentName": "country",
	"title": "Country",
	"componentClass": "es-country",
	"componentCustomJsClass": "js-es-block-form-custom",
	"componentJsSingleSubmitClass": "js-es-block-form-single-submit",
	"example": {
		"attributes": {
			"countryName": "Name",
			"countryIsDisabled": false,
			"countryIsRequired": false
		}
	},
	"components": {
		"field": "field",
		"conditionalTags": "conditionalTags"
	},
	"attributes": {
		"countryDatasetUsed": {
			"type": "string",
			"default": "default"
		},
		"countrySelectedValue": {
			"type": "string",
			"default": "us"
		},
		"countryName": {
			"type": "string"
		},
		"countryIsDisabled": {
			"type": "boolean",
			"default": false
		},
		"countryIsRequired": {
			"type": "boolean",
			"default": false
		},
		"countryTracking": {
			"type": "string"
		},
		"countryMeta": {
			"type": "string"
		},
		"countryAttrs": {
			"type": "object"
		},
		"countryDisabledOptions": {
			"type": "array"
		},
		"countryUseSearch": {
			"type": "boolean",
			"default": true
		}
	}
}
