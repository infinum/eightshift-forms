{
	"$schema": "https://raw.githubusercontent.com/infinum/eightshift-frontend-libs/develop/schemas/component.json",
	"componentName": "submit",
	"title": "Submit",
	"componentClass": "es-submit",
	"example": {
		"attributes": {
			"submitValue": "Value",
			"submitType": "button",
			"submitIsDisabled": false
		}
	},
	"components": {
		"field": "field"
	},
	"attributes": {
		"submitUniqueId": {
			"type": "string"
		},
		"submitName": {
			"type": "string"
		},
		"submitButtonComponent": {
			"type": "string"
		},
		"submitValue": {
			"type": "string",
			"default": "Submit"
		},
		"submitIsDisabled": {
			"type": "boolean",
			"default": false
		},
		"submitButtonAsLink": {
			"type": "boolean",
			"default": false
		},
		"submitButtonAsLinkUrl": {
			"type": "string"
		},
		"submitTracking": {
			"type": "string"
		},
		"submitAttrs": {
			"type": "object"
		},
		"submitServerSideRender": {
			"type": "boolean",
			"default": false
		},
		"submitMeta": {
			"type": "string"
		},
		"submitIcon": {
			"type": "string"
		},
		"submitDisabledOptions": {
			"type": "array"
		},
		"submitVariant": {
			"type": "string",
			"default": "default"
		}
	}
}
